using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace wayeal.os.exhaust
{
    /// <summary>
    /// 系统参数信息
    /// </summary>
    class SystemParamInfo
    {
        /// <summary>
        /// 其他参数信息，只包括站点信息，不包括开机信息
        /// </summary>
        public class OtherParamInfo
        {
            /// <summary>
            /// 站点编号
            /// </summary>
            public string StationNumber { get; set; }
            /// <summary>
            /// 站点名称
            /// </summary>
            public string Name { get; set; }
            /// <summary>
            /// 站点类型
            /// </summary>
            public int StationType { get; set; }
            /// <summary>
            /// 经度
            /// </summary>
            public string Longitude { get; set; }
            /// <summary>
            /// 纬度
            /// </summary>
            public string Latitude { get; set; }
            /// <summary>
            /// 高程
            /// </summary>
            public double Highly { get; set; }
            /// <summary>
            /// 坡度
            /// </summary>
            public double Slope { get; set; }
            /// <summary>
            /// 详细地址
            /// </summary>
            public string DetialedAddress { get; set; }
            /// <summary>
            /// 备注
            /// </summary>
            public string Tips { get; set; }
        }
    }
}
